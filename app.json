{
  "expo": {
    "owner":"dangerro",
    "name": "busylittlemobileapp",
    "slug": "busylittlemobileapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/images/blp-splash.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./app/assets/images/icon-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "jsEngine": "jsc",
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.busylittlemobileapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "blp.mobile.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./app/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "157ae5e2-48bc-4393-b3bf-fe5deb1a0980"
      }
    }
  }
}
